{
  "name": "example-idx-agent-extension",
  "displayName": "Example IDX Agent Extension",
  "description": "An example of an IDX agent extension",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.83.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "monospacePlugin": {
    "apiVersion": "1",
    "features": [
      "ai"
    ]
  },
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:esbuild": "node esbuild.js --watch",
    "dev:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@firebase-studio/plugin-sdk": "github:romannurik/temp-studio-plugin-api",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.1",
    "@vscode/vsce": "^2.26.1",
    "esbuild": "^0.20.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3"
  }
}
